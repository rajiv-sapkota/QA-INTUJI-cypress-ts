{
  "dependencies": {
    "cypress": "^14.4.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^22.15.30",
    "cypress-mochawesome-reporter": "^3.8.2",
    "dotenv": "^16.5.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
  "cypress:run": "cypress run",
  "report:merge": "mochawesome-merge cypress/reports/mochawesome/*.json > cypress/reports/mochawesome/mochawesome.json",
  "report:generate": "marge cypress/reports/mochawesome/mochawesome.json --reportDir cypress/reports/html",
  "test": "npm run cypress:run && npm run report:merge && npm run report:generate"
}
}
